{
  "name": "lisan-alhekma-client",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev",
    "dev:staging": "dotenv -e .env.staging -- cross-env NODE_ENV=development next dev",
    "dev:production": "dotenv -e .env.production -- cross-env NODE_ENV=development next dev",
    "build": "dotenv -e .env.local -- next build",
    "build:staging": "dotenv -e .env.staging -- cross-env NODE_ENV=production next build",
    "build:production": "dotenv -e .env.production -- cross-env NODE_ENV=production next build",
    "start": "next start",
    "start:staging": "dotenv -e .env.staging -- cross-env NODE_ENV=production next start",
    "start:production": "dotenv -e .env.production -- cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "health": "curl http://localhost:3000/api/health",
    "test:config": "node test-config.js"
  },
